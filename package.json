    "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start:staging": "dotenv -e .env.staging react-scripts start",
    "build:staging": "dotenv -e .env.staging react-scripts build",
    "start:local": "dotenv -e .env.local react-scripts start",
    "build:local": "dotenv -e .env.local react-scripts build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm start\" \"npm run electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-big-calendar": "^1.8.9",
